  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "angular": "1.6.1",
    "angular-ui-router": "0.4.2"
  },
  "description": "Front-end workflow for angular 1 projects based on webpack",
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "3.1.2",
    "babel-preset-latest": "6.22.0",
    "babel-register": "6.22.0",
    "browser-sync": "2.18.7",
    "chai": "3.5.0",
    "clean-webpack-plugin": "0.1.15",
    "commitizen": "2.9.5",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "htmllint": "0.6.0",
    "htmllint-cli": "0.0.6",
    "htmllint-loader": "1.3.8",
    "husky": "0.13.1",
    "mocha": "3.2.0",
    "ng-annotate-loader": "0.2.0",
    "node-sass": "4.5.0",
    "npm-check": "5.4.0",
    "nyc": "10.1.2",
    "sass-lint": "1.10.2",
    "sass-loader": "4.1.1",
    "sasslint-webpack-plugin": "1.0.4",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-notifier": "1.5.0"
  },
  "homepage": "https://github.com/gabortill/angular-1-webpack-workflow#readme",
  "keywords": [
    "angular 1",
    "webpack"
  ],
  "license": "ISC",
  "main": "index.js",
  "name": "angular-1-webpack-workflow",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabortill/angular-1-webpack-workflow.git"
  },
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "check:dependencies": "npm-check --skip-unused",
    "build": "webpack --optimize-minimize --progress",
    "build:open": "npm run build && browser-sync start --server dist",
    "commit": "git-cz",
    "lint:all": "npm run lint:html & npm run lint:js & npm run lint:sass",
    "lint:html": "htmllint --cwd src",
    "lint:js": "eslint src/**/*.js",
    "lint:sass": "sass-lint src/**/*.scss",
    "prepush": "npm run lint:all && npm test",
    "start": "webpack-dev-server --progress --colors --open --inline --hot --history-api-fallback --content-base bin/",
    "test": "NODE_ENV=test nyc mocha src/**/*.test.js"
  },
  "version": "1.0.0"
}
